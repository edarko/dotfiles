! General -----------------------------------------
 
urxvt.termName:         rxvt
urxvt.loginShell:       true
urxvt.scrollBar:        false
urxvt.secondaryScroll:  true
urxvt.saveLines:        65535
urxvt.cursorBlink:      true
urxvt*font:             xft:DejaVu Sans Mono:pixelsize=11
urxvt*boldFont:         xft:DejaVu Sans Mono:pixelsize=11
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher,searchable-scrollback
urxvt*urlLauncher:      /usr/bin/firefox
urxvt*matcher.button:   1
urxvt*cutchars:         "()'*<>[]{|}"
urxvt.cursorColor:      #dcdcdc
urxvt*geometry:         95x25
urxvt.urgentOnBell:     false
urxvt.visualBell:       false
URxvt.depth: 		32
URxvt.background: 	[85]#000000
!urxvt.borderLess:      false
!urxvt.borderWidth:     1
!urxvt.internalBorder:  0
 
! Colors ------------------------------------------
 
*foreground:            #ffffff
*background:            #000000
!black
*color0:                #000000
*color8:                #777777
!red
*color1:                #cd0000
*color9:                #ff0000
!green
*color2:                #b1d631
*color10:               #b1d631
!yellow
*color3:                #ff8700
*color11:               #ff8700
!blue
*color4:                #396398
*color12:               #396398
!magenta
*color5:                #b7416e
*color13:               #b7416e
!cyan
*color6:                #577377
*color14:               #577377
!white
*color7:                #b5b5b5
*color15:               #ffffff

urxvt --help 2>&1| sed -n '/:  /s/^ */! URxvt*/gp' >> ~/.Xresources
